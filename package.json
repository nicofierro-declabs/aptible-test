{
  "name": "conneqt-backend",
  "version": "0.0.1",
  "description": "Conneqt backend",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon ./dist/index.js",
    "dev": "nodemon --exec ts-node -- ./src/index.ts",
    "prod": "npm run build && npm run start",
    "clean": "rm -rf node_modules coverage dist .nyc_output",
    "lint": "eslint ./src/**/*.ts --fix",
    "migrate:make": "knex migrate:make --knexfile ./src/config/db/knexfile.ts -x ts",
    "migrate:latest": "knex migrate:latest --knexfile ./src/config/db/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile ./src/config/db/knexfile.ts",
    "seed:make": "knex seed:make --knexfile ./src/config/db/knexfile.ts -x ts",
    "seed:run": "knex seed:run --knexfile ./src/config/db/knexfile.ts",
    "test": "mocha -r ts-node/register 'tests/**/*.test.ts'"
  },
  "keywords": [],
  "author": "December Labs",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.19",
    "knex-migrate": "^1.7.4",
    "luxon": "^2.0.2",
    "mocha": "^9.1.1",
    "nodemailer": "^6.6.3",
    "objection": "^2.2.15",
    "pg": "^8.6.0",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "speakeasy": "^2.0.0",
    "twilio": "^3.67.1",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.22",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/luxon": "^2.0.2",
    "@types/mocha": "^9.0.0",
    "@types/mock-knex": "^0.4.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.3.3",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.1",
    "@types/qrcode": "^1.4.1",
    "@types/speakeasy": "^2.0.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mock-knex": "^0.4.10",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
